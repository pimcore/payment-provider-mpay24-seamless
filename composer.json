{
  "name": "pimcore/payment-provider-mpay24-seamless",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "description": "Pimcore Payment Provider - MPay24 Seamless",
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "mpay24/mpay24-php": "^4.2",
    "pimcore/pimcore": "^11.0"
  },
  "conflict": {
    "pimcore/pimcore": "v11.0.0-ALPHA1 || v11.0.0-ALPHA2"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Pimcore\\Bundle\\EcommerceFrameworkBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pimcore\\Model\\DataObject\\": "var/classes/DataObject"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Pimcore\\Bundle\\EcommerceFrameworkBundle\\PimcorePaymentProviderMpay24SeamlessBundle"
      ]
    }
  }
}
